<%
  case ENV['RAILS_ENV']
  when 'test'
    _mp = 'myprofile-test'
  when 'staging'
    _mp = 'myprofile-test'
  when 'development'
    _mp = 'myprofile-development'
  else
    _mp = 'myprofile'
  end
%>

window.msettings =
  audience: '<%= ENV['AUDIENCE'] %>'
  surveyMarkup: ''
  environment: '<%= ENV['RAILS_ENV'] %>'
  myprofileName: '<%= _mp %>'

jQuery ($) ->
  $.cookie.json = true
