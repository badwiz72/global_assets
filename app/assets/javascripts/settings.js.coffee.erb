<%
  case ENV['RAILS_ENV']
  when 'test'
    _mp = 'myprofile-test'
  when 'staging'
    _mp = 'myprofile-test'
  when 'development'
    _mp = 'myprofile-development'
  else
    _mp = 'myprofile'
  end
%>

window.msettings =
  audience: '<%= ENV['AUDIENCE'] %>'
  surveyMarkup: ''
  environment: '<%= ENV['RAILS_ENV'] %>'
  myprofileName: '<%= _mp %>'

# Get Universal Analytics masked user id from cookie
try
  window.muaid = JSON.parse(decodeURIComponent(
    document.cookie.replace(new RegExp("(?:(?:^|.*;)\\s*" +
    encodeURIComponent(msettings.myprofileName).replace(/[\-\.\+\*]/g, "\\$&") +
    "\\s*\\=\\s*([^;]*).*$)|^.*$"), "$1"))).ua;
catch
  window.muaid = null

jQuery ($) ->
  $.cookie.json = true
  return
